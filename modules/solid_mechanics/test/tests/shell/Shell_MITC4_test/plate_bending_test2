[Preconditioning]
  [./smp]
    type = SMP
    full = true
  [../]
[]

[Executioner]
  type = Transient
  solve_type = NEWTON
   petsc_options = '-snes_ksp_ew'
  # best overall
  petsc_options_iname = '-pc_type -pc_factor_mat_solver_package'
   petsc_options_value = ' lu       mumps'
  line_search = 'none'
  nl_rel_tol = 1e-13
  nl_abs_tol = 1e-15
  dt = 1
  end_time = 10

[]

[Kernels]
  [./solid_disp_x]
    type = ADStressDivergenceShell2
    component = 0
    variable = disp_x
