[Materials]
  [./elasticity]
    type = ADComputeIsotropicElasticityTensorShell
    youngs_modulus = 1e4
    poissons_ratio = 0.3
    through_thickness_order = SECOND
  [../]
  [./strain]
    type = ADComputeIncrementalShellStrain2
    displacements = 'disp_x disp_y disp_z'
    rotations = 'rot_x rot_y'
    thickness = 0.5
    through_thickness_order = SECOND
  [../]
  [./stress]
    type = ADComputeShellStress2
    through_thickness_order = SECOND
  [../]
[]

